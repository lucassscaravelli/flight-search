{
  "extends": ["eslint-config-airbnb"],
  "env": {
    "browser": true,
    "jasmine": true,
    "jest": true
  },
  "rules": {
    "linebreak-style": 0,
    "import/prefer-default-export": "off",
    "react/jsx-sort-props": [
      "error",
      {
        "callbacksLast": true,
        "reservedFirst": ["key"]
      }
    ],
    "react/sort-prop-types": [
      "error",
      {
        "callbacksLast": true,
        "requiredFirst": true
      }
    ],
    "implicit-arrow-linebreak": "off"
  }
}
